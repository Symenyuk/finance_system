{
  "extends": "../../tslint.json",
  "rules": {
    "no-string-literal": false,
    "directive-selector": [
      true,
      "attribute",
      "lib",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "lib",
      "kebab-case"
    ],
    "variable-name": [
      "allow-leading-underscore"
    ]
  }
}
